<% provide(:title, "Movie manager") %>
<ol class="breadcrumb text-left">
  <li>
    <%= link_to admin_root_path do %>
      <span class="glyphicon glyphicon-home"></span>
    <% end %>
  </li>
  <li class="active">
    <%= link_to t("sidebar.movies_manager"), admin_movies_path %>
  </li>
</ol>
<div class="col-xs-12">
  <div class="admin-list-movies" id="movies">
    <%= render "movies", support: @support %>
  </div>
</div>
